                    <a class="CoveoTab" data-id="All" data-caption="AllContent"> </a>
                    <a class="CoveoTab" data-id="KB" data-caption="Knowledge Base"
                        data-expression="@infadocumenttype==(KB,IPSKB)"> </a>
                    <a class="CoveoTab" data-id="ProdDocs" data-caption="Product Docs"
                        data-expression="@athenatabname==&quot;Product Docs&quot;"> </a>
                    <!-- <T07>  -->
                    <a class="CoveoTab" data-id="HowTo" data-caption="How-to Library"
