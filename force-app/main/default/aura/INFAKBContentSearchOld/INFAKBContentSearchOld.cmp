    access="global">
    <!--T04--><!--T05--><ltng:require scripts="{!$Resource.CoveoJsFrameworkJsMinNew + '/CoveoJsFrameworkJsMin/CoveoJsSearch.js,'+        
    $Resource.INFAKBSearchOld + '/Js/InfaKBCommonUtility.js,'+        
    $Resource.INFAKBSearchOld + '/Js/CustomCoveoTemplateFramework.js,'+
    $Resource.INFAKBSearchOld + '/Js/InfaKBSearchTemplateUtility.js,'+
    $Resource.INFAKBSearchOld + '/Js/InfaKBContentVisitedFeature.js,'+
    $Resource.INFAKBSearchOld + '/Js/scode.js,'+
    $Resource.INFAKBSearchOld + '/Js/Woopra_Old.js,'+
    $Resource.INFAKBSearchOld + '/Js/Omniture_Old.js,'+
    $Resource.INFAKBSearchOld + '/Js/Omniture_DTM.js,'+
    $Resource.INFAKBSearchOld + '/Js/InfaKBSearchOld.min.js'}" styles="{!$Resource.CoveoJsFrameworkOthersNew + '/CoveoJsFrameworkOthers/css/CoveoFullSearch.css,'+       
    $Resource.INFAKBSearchOld + '/Css/AthenaKBSearchCSS.css,'+
    $Resource.INFAKBSearchOld + '/Css/wrapper.css,'+
    $Resource.INFAKBSearchOld + '/Css/clientlibs.css,'+
    $Resource.INFAKBSearchOld + '/Css/INFAKBContentSearchOld.css,'+
    $Resource.INFAKBSearchOld + '/Css/font-awesome.min.css'}" afterScriptsLoaded="{!c.onScriptLoaded}" />
    <aura:attribute name="currentUserFedId" type="User" default="" />
    <force:recordData aura:id="recordLoader" recordId="{!$SObjectType.CurrentUser.Id}"
        fields="FirstName,FederationIdentifier" targetFields="{!v.currentUserFedId}" />
    <aura:handler name="init" value="this" action="{!c.doInit}" />
    <lightning:workspaceAPI aura:id="workspace" />
    <aura:attribute name="hdnCurrentSessionId" type="String" default="" />
    <aura:attribute name="hdnUserFilter" type="String" default="" />
    <aura:attribute name="hdnSFDCKBExternalHost" type="String" default="{!$Label.c.KB_External_Host}" />
    <aura:attribute name="hdnSFDCKBInternalHost" type="String" default="{!$Label.c.KB_Internal_Host}" />
    <aura:attribute name="hdnKBCommunityNameInURL" type="String" default="{!$Label.c.KB_Community_Name_In_URL}" />
    <aura:attribute name="hdneSupportCommunityNameInURL" type="String"
        default="{!$Label.c.eSupport_Community_Name_In_URL}" />
    <aura:attribute name="hdnKBNetworkSignUpURL" type="String" default="{!$Label.c.CustomerSupportSignupURL}" />
    <aura:attribute name="hdnKBNetworkLoginURL" type="String" default="{!$Label.c.CustomerSupportLoginURL}" />
    <aura:attribute name="hdnKBSearchTokenValidity" type="String" required="true" default="{!$Label.c.KB_Search_Token_Validity}" /><!-- <T06>  -->
    <aura:attribute name="PlacedIn" type="String" required="true" default="contentsearch" />


