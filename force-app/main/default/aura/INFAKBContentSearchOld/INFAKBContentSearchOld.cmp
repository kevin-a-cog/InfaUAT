                                data-add-search-button="true" data-enable-wildcards="true" data-enable-omnibox="true"
                                data-enable-simple-addon="false" data-enable-field-addon="false"
                                data-enable-top-query-addon="true" data-enable-reveal-query-suggest-addon="true"
                                data-enable-query-extension-addon="false" data-trigger-query-on-clear="true"></div>
                        </div>
                        <!-- <div class="input-group-btn">
                            <button type="button" class="btn btn-default btn-search CoveoSearchButton">Search</button>
                        </div> -->
                    </div>
                    <div class="CoveoShareQuery"></div>
                    <div class="CoveoExportToExcel"></div>
                    <div class="CoveoPreferencesPanel">
                        <div class="CoveoResultsPreferences"></div>
                        <div class="CoveoResultsFiltersPreferences"></div>
                    </div>
                </div>
            </div>
            <div class="tab-section-parent">
