            <lightning:spinner alternativeText="Loading" size="medium" />
        </div>
    </div>
    <div class="slds-card" style="display: none;">
        <div id="searchinfakbcs" class="CoveoSearchInterface" data-enable-history="false" data-design="new"
            data-excerpt-length="250" data-always-open-in-new-window="true">
