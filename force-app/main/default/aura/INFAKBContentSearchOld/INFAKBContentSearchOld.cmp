            <!-- <T05>  -->
            <div class="CoveoFoldingForThread" data-field="@foldfoldingfield" data-parent="@foldchildfield"
                data-child="@foldparentfield" data-maximum-expanded-results="4" data-range="1" data-tab="All,Blog">
            </div>
            <!-- </T05>  -->
            <!-- <T05>  -->
            <div class="CoveoFoldingForThread" data-field="@foldfoldingfield" data-parent="@foldchildfield"
