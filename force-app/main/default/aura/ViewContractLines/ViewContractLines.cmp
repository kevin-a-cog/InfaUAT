<!--Lightning Component to Display the Contract Line records related to a Renewal Quote Design-->
<!-- ================================================================== -->
<!-- PLEASE KEEP THE FORMATTING CLEAN, MAKES DEBUGGING EASY, THANK YOU -->
<!-- Controller="ViewContractLinesController"  ====================== -->
<!-- Added PRIOR UPLIFTRENEWAL NOTES -->
<!-- Added Renewal Year,Offering Id,Description and Old Org Order Number colums (SALESRT-11796) <T01>-->
<!-- Added Download button to export the column (SALESRT-11773) <T02>
     NOTE: ADD THE AURD-ID TAG WITH THE VALUES PriorcellData/OriginalcellData TO ALL THE TABLE CELL DATA, TO BE
           FETECHED FOR THE DOWNLOAD

-->

<aura:component controller="ViewContractLinesController" implements="force:appHostable,flexipage:availableForAllPageTypes,force:hasRecordId,force:lightningQuickActionwithoutHeader" access="global">
    <!-- ATTRIBUTES + HANDLERS -->
    <!-- ====================================================================================================================================================== -->
    <aura:attribute name="loading" type="Boolean" default="true" />
    <aura:attribute name="showTopTable" type="Boolean" default="true" />
    <aura:attribute name="noAssets" type="Boolean" default="false" />
    <aura:attribute name="assetWrapperList" type="List" />
    <aura:attribute name="originSObjectName" type="String" />
    <aura:attribute name="error" type="String" />
    <aura:attribute name="today" type="Date" default=""/>
    <aura:attribute name="QuoteRec" type="Object"/>
    
    <aura:handler name="init" value="{!this}" action="{!c.doInit}"/>
    
    <aura:html tag="style">
        .slds-modal__container{
        width:100%;
        max-width:100% ;
        height: 100%;
        }
    </aura:html>  
    
    <!--To get the Quote record Data without query-->
    <force:recordData layoutType="FULL"
                      recordId="{!v.recordId}"
                      targetFields="{!v.QuoteRec}"
                      mode="VIEW"/>   
    
    <!-- MAIN SECTION -->
    <!-- ====================================================================================================================================================== -->
    <section role="dialog" tabindex="-1" class="slds-modal slds-fade-in-open slds-modal_large" aria-labelledby="id-of-modalheader-h2" aria-describedby="modal-content-id-1">
        <div class="slds-modal__container">
            
            <!-- MODAL HEADER -->
            <!-- =============================================================================== -->
            <header class="slds-modal__header">
                <button class="slds-button slds-button_icon slds-modal__close slds-button_icon-inverse" title="Close" onclick="{!c.closeModalOne}">
                    <lightning:icon class="slds-button__icon slds-button__icon_large fillColor" iconName="utility:close" size="x-small" alternativeText="search"/>
                    <span class="slds-assistive-text">Close</span>
                </button>
                <h2 id="id-of-modalheader-h2" class="slds-text-heading_medium">Quote Lines and Contract Lines comparison</h2>
            </header>
            
            <!--TOGGLE BUTTON-->
            <div class="slds-clearfix" >
                <div class="slds-float--right">
                    <div class="slds-form-element">
                        <label class="slds-checkbox_toggle slds-grid">
                            <span class="slds-form-element__label slds-m-bottom_none">Prior Values</span>
                            <input type="checkbox" name="checkbox-toggle-19" value="checkbox-toggle-19" aria-describedby="checkbox-toggle-19" change="{!c.toggle}"/>
                            <span id="checkbox-toggle-19" class="slds-checkbox_faux_container" aria-live="assertive" onclick="{!c.toggle}">
                                <span class="slds-checkbox_faux" style="background-color:rgba(27, 82, 151, 1)"></span>
                            </span>
                            <span class="slds-form-element__label slds-m-bottom_none" style="padding-left:8px;">Original Values</span>
                        </label>
                    </div>
                    <!--<lightning:button class="slds-button slds-button_brand " label="Toggle" onclick="{!c.toggle}"/>-->
                </div>
            </div>
            
