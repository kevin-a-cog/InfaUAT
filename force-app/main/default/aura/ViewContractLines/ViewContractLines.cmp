                                                </aura:if>
                                                <td class="slds-text-align_center" >
                                                    <ui:outputText value="{!assetWrapper.quantity}" class="slds-text-align_center"/>
                                                </td>
                                                <td class="slds-text-align_center" >
                                                    <lightning:formattedNumber value="{!assetWrapper.listPrice}" style="currency" currencyCode="USD" currencyDisplayAs="code"/>
                                                </td>
                                                <td class="slds-text-align_center" >
                                                    <lightning:formattedNumber value="{!assetWrapper.extendedPrice}" style="currency" currencyCode="USD" currencyDisplayAs="code"/>
                                                </td>
                                                <td class="slds-text-align_center" > <!--<T01>-->
                                                    <lightning:formattedNumber value="{!assetWrapper.quoteLineRenewalYear}"/>
                                                </td>
                                                <td class="slds-text-align_center" >
                                                    <ui:outputText value="{!assetWrapper.quoteLineOfferingID}" class="slds-text-align_center"/>
                                                </td>
                                                <td class="slds-text-align_left" >
                                                    <ui:outputTextArea value="{!assetWrapper.quoteLineDescription}" class="slds-text-align_center"/>
                                                </td>                                                                     
                                                <td class="slds-text-align_center" >
                                                    <a href="{!'/one/one.app?#/sObject/'+ assetWrapper.originalContractId+ '/view'}" target="_blank"><ui:outputText aura:id ='OriginalcellData' value="{!assetWrapper.originalContractName}" class="slds-text-body--regular"/></a>
                                                </td>
                                                <td class="slds-text-align_center" >
                                                    <a href="{!'/one/one.app?#/sObject/'+ assetWrapper.originalContractLineId + '/view'}" target="_blank"><ui:outputText aura:id ='OriginalcellData' value="{!assetWrapper.originalContractLineName}" class="slds-text-body--regular"/></a>
                                                </td>
                                                <td> <!--<T01>-->
                                                    <ui:outputText aura:id ='OriginalcellData' value="{!assetWrapper.originalOldOrgOrderNumber}"/>
                                                </td>
                                                <td>
                                                    <ui:outputText aura:id ='OriginalcellData' value="{!assetWrapper.originalProductName}"/>
                                                </td>
                                                <td class="slds-text-align_center" >
                                                    <ui:outputText aura:id ='OriginalcellData' value="{!assetWrapper.originalSubscriptionTerm}" class="slds-text-align_center"/>
                                                </td>
                                                <td class="slds-text-align_center" >
                                                    <ui:outputDate aura:id ='OriginalcellData' value="{!assetWrapper.originalStartDate}" class="slds-text-align_center"/>
                                                </td>
                                                <td class="slds-text-align_center" >
                                                    <ui:outputDate aura:id ='OriginalcellData' value="{!assetWrapper.originalEndDate}" class="slds-text-body--regular"/>
                                                </td>
                                                <td class="slds-text-body--regular" >
                                                    <ui:outputText aura:id ='OriginalcellData' value="{!assetWrapper.originalQuantity}" class="slds-text-align_center"/>
                                                </td>
                                                <td class="slds-text-align_center" >
                                                    <lightning:formattedNumber aura:id ='OriginalcellData' value="{!assetWrapper.originalListPrice}" style="currency" currencyCode="USD" currencyDisplayAs="code"/>
                                                </td>
                                                <td class="slds-text-align_center" >
                                                    <lightning:formattedNumber aura:id ='OriginalcellData' value="{!assetWrapper.originalExtendedPrice}" style="currency" currencyCode="USD" currencyDisplayAs="code"/>
                                                </td>
                                            </tr>
                                        </aura:iteration>
                                    </tbody>
                                </table>
                            </div>
                            
